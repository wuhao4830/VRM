<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lsh.base.data.bean"><!-- 这里的package记得要写对啊。要不然运行也会出错的 -->
    <class name="Delivery" table="item_delivery"><!-- 如果表名跟类名一个样的话，那么table属性可以不写 -->
        <id name="id" >
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="marketId" column="market_id"/>
        <property name="skuId" column="sku_id"/>
        <property name="kunnr" column="kunnr"/>
        <property name="werks" column="werks"/>
        <property name="maktx" column="maktx"/>
        <property name="mmstaTxt" column="mmstaTxt"/>
        <property name="zkhspbm" column="zkhspbm"/>
        <property name="lbkum" column="lbkum"/>
        <property name="meins" column="meins"/>
        <property name="zdate" column="zdate"/>
        <property name="ztime" column="ztime"/>
        <property name="zuname" column="zuname"/>
    </class>
</hibernate-mapping>
        